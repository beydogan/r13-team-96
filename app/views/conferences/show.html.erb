  <% @conference.organizations.each do |organization| %>
    <%= organization.name %>
  <% end %>
</p>

<p>
  <b>Name:</b>
  <%= @conference.name %>
</p>

<p>
  <b>Summary:</b>
  <%= @conference.summary %>
</p>

